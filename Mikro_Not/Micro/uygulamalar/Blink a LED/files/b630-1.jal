include f675_20 -- no 16F630/676 support yet, use 12F675
include jlib     

var bit LED1 is pin_a0
var bit LED2 is pin_c0
pin_a0_direction = output
pin_c0_direction = output

forever loop
  delay_100ms( 5 )
  LED1 = high
  LED2 = high
  delay_100ms( 5 )
  LED1 = low
  LED2 = low
end loop
